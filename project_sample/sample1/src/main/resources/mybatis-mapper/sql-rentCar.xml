<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.sample1.mapper.RentCarMapper">
	<select id="selectRentCarList" parameterType="hashmap" resultType="com.example.sample1.model.Product">
		SELECT *
		FROM T2_RENT_CAR_TBL R
		INNER JOIN T2_CODE_TYPE C ON R.RENT_KIND = C.C_KIND
	</select>
	
	<delete id="deleteRentCar" parameterType="hashmap">
		DELETE
		FROM T2_RENT_CAR_TBL
		WHERE RENT_NO = #{rentNo}
	</delete>
</mapper>